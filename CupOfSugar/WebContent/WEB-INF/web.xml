<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <servlet-name>InsertedRequestServlet</servlet-name>
    <servlet-class>login.InsertedRequestServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ListDao</servlet-name>
    <servlet-class>login.ListDao</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AcceptRequestDao</servlet-name>
    <servlet-class>login.AcceptRequestDao</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AcceptRequestServlet</servlet-name>
    <servlet-class>login.AcceptRequestServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>InsertRequestServlet</servlet-name>
    <servlet-class>login.InsertRequestServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>login.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoggedInServlet</servlet-name>
    <servlet-class>login.LoggedInServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SignupServlet</servlet-name>
    <servlet-class>login.SignupServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SignupServlet</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoggedInServlet</servlet-name>
    <url-pattern>/loggedin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>InsertRequestServlet</servlet-name>
    <url-pattern>/insertrequest</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AcceptRequestServlet</servlet-name>
    <url-pattern>/acceptrequest</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>InsertedRequestServlet</servlet-name>
    <url-pattern>/saved</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ListDao</servlet-name>
    <url-pattern>/list</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>